<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ProductService"
                   assembly="ProductService">

  <class name="Address" table="Addreses" lazy="false">

    <!-- Identity mapping -->
    <id name="ID">
      <column name="AddressID" />
      <generator class="native" />
    </id>

    <property name="Street" />
    <property name="State" />
    <property name="City" />
    <property name="ZIP" />

  </class>

</hibernate-mapping>
